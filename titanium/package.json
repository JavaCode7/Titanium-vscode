{
    "name": "titanium",
    "displayName": "Titanium",
    "description": "A simple VSCode extension for the Titanium esoteric programming language.",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.53.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "titanium",
            "aliases": ["Titanium", "titanium"],
            "extensions": [".tix",".titanium"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "titanium",
            "scopeName": "",
            "path": "./syntaxes/titanium.tmLanguage.json"
        }]
    }
}